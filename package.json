{
  "name": "skult",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "eslint --fix \"src/**/*.{js,jsx,tsx,ts}\"",
    "prettier-check": "node_modules/.bin/prettier ./.prettierrc.js --check 'src/**/*.{js,jsx,ts,tsx}' --no-error-on-unmatched-pattern",
    "prettier-run": "node_modules/.bin/prettier ./.prettierrc.js --write \"src/**/*.{js,jsx,ts,tsx}\" --no-error-on-unmatched-pattern",
    "co:login:skultgg": "aws codeartifact login --tool npm --repository lib --domain skultgg --domain-owner 129095079376 --namespace @skultgg",
    "co:login:skultgg-config": "aws codeartifact login --tool npm --repository eslint-config-skult --domain skultgg --domain-owner 129095079376 --namespace @skultgg-config",
    "co:login": "npm run co:login:skultgg && npm run co:login:skultgg-config"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,md,html,css,scss,json,yml}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --quiet --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.1",
    "@skultgg/lib": "^1.23.0",
    "dotenv": "^16.0.0",
    "framer-motion": "^6.2.9",
    "moment": "^2.29.1",
    "next": "^12.1.0",
    "notistack": "^2.0.4",
    "react": "^17.0.2",
    "react-background-video-player": "^1.1.8",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^8.33.1",
    "react-material-ui-carousel": "^3.3.3",
    "react-native-google-places-autocomplete": "^2.4.1",
    "react-responsive": "^9.0.0-beta.6"
  },
  "devDependencies": {
    "@devexpress/dx-react-chart": "^2.7.6",
    "@devexpress/dx-react-chart-material-ui": "^2.7.6",
    "@material-ui/core": "^4.12.3",
    "@skultgg-config/eslint-config-skult": "^1.5.0",
    "@syncfusion/ej2-react-charts": "^19.4.54",
    "@types/node": "^16.11.26",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.14",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/react-places-autocomplete": "^7.2.9",
    "@types/react-scroll": "^1.8.3",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^38.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  }
}
